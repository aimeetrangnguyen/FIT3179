{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": "Ramen ratings in Asia",
  "width": 800,
  "height": 600,
  "data": {
    "url": "https://raw.githubusercontent.com/aimeetrangnguyen/FIT3179/master/js/%20asia1.geo.json",
    "format": {
      "type": "topojson",
      "feature": "asia"
    }
  },
  "mark": {
    "type": "geoshape"
  },
  "transform": [
    {
      "lookup": "properties.name",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/aimeetrangnguyen/FIT3179/master/data/ramen_normalise.csv"
        },
        "key": "Country",
        "fields": [
          "Average"
        ]
      }
    }
  ],
  "projection": {
    "type": "equirectangular"
  },
  "encoding": {
    "color": {
      "field": "Average",
      "type": "quantitative"
    },
    "tooltip": [
      {
        "field": "properties.name",
        "type": "nominal",
        "title": "Country"
      },
      {
        "field": "Average",
        "type": "quantitative",
        "format": ".2f"
      }
    ]
  },
  "config": {}
}